{
  "name": "vite-react",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node server.dev.js",
    "build": "rimraf ../../dist/vite-react/* && vue-tsc --noEmit && yarn build:client && yarn build:server",
    "build:client": "vite build --ssrManifest --outDir ../../dist/vite-react/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir ../../dist/vite-react/server",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-legacy": "^1.8.0",
    "typescript": "^4.5.4",
    "rimraf": "^3.0.2",
    "vite": "^2.9.0"
  }
}
